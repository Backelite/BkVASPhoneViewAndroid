<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="VasPhoneView">
        <!-- vasPhoneViewSize can take values: normal (default), small-->
        <attr name="VasPhoneViewSize" format="enum">
            <enum name="normal" value="0" />
            <enum name="small" value="1" />
        </attr>
        <!-- vasPhoneViewStyle can take values: free, standard, chargeable-->
        <attr name="VasPhoneViewStyle" format="enum">
            <enum name="free" value="0" />
            <enum name="standard" value="1" />
            <enum name="chargeable" value="2" />
        </attr>

    </declare-styleable>

</resources>